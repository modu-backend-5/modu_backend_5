   "source": [
    "a, b = map(int, input().split())\n",
    "\n",
    "a -= 1\n",
    "b -= 1\n",
    "\n",
    "row1 = a // 4\n",
    "row2 = b // 4\n",
    "row = abs(row1 - row2)\n",
    "\n",
    "col1 = a % 4\n",
    "col2 = b % 4\n",
    "column = abs(col1 - col2)\n",
    "\n",
    "dist = row + column\n",
    "\n",
    "print(dist)"
   ]
  }
