    "aeiou = [\"a\", \"e\", \"i\", \"o\", \"u\"]\n",
    "\n",
    "while True:\n",
    "    cnt = 0\n",
    "    sentence = input()\n",
    "    \n",
    "    if sentence == \"#\":\n",
    "        break\n",
    "    \n",
    "    sentence = sentence.lower()\n",
    "    \n",
    "    for i in range(len(sentence)):\n",
    "        if sentence[i] in aeiou:\n",
    "            cnt += 1\n",
    "    \n",
    "    print(cnt)"
   ]
