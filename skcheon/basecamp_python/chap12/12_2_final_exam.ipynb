    "# 기말고사 3번\n",
    "class User:\n",
    "    \n",
    "    def __init__(self, _id, age, eyeColor, name, gender):\n",
    "        self._id = _id\n",
    "        self.age = age\n",
    "        self.eyeColor = eyeColor\n",
    "        self.name = name\n",
    "        self.gender = gender\n",
    "    \n",
    "    def introduce(self):\n",
    "        print(f\"안녕하세요. 제 이름은 {self.name}이고, 나이는 {self.age}세입니다. 눈 색깔은 {self.eyeColor}이고, 젠더는 {self.gender}입니다.\")\n",
    "        \n",
    "    \n",
    "amy = User(\"0111\", 20, \"brown\", \"Amy\", \"female\")\n",
    "amy.introduce()"
   ]
